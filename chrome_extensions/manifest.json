{
  "manifest_version": 3,
  "name": "Qsuggest",
  "version": "1.0",
  "description": "Qsuggest is a tool designed to help users enhance their competitive programming skills by recommending suitable practice problems based on their history and performance in codeforces.",
  "content_scripts": [
    {
      "matches": [
        "*://codeforces.com/problemset",
        "*://codeforces.com/problemset?*"
      ],
      "js": [
        "scripts/sidebar.js",
        "scripts/load_data.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "host_permissions": [
    "http://127.0.0.1:5000/"
  ]
}